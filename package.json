    "angular-growl-v2": "github:Jacq/angular-growl-2",
    "angular-messages": "1.8.2",
    "angular-resource": "1.8.2",
    "angular-route": "1.8.2",
    "angular-sanitize": "1.8.2",
    "angular-spinner": "0.6.2",
    "angular-touch": "1.8.2",

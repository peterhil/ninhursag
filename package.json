{
  "name": "ninhursag",
  "description": "Ninhursag is a visualisation of US Geologic Survey statistics from year 1900 onwards of world production of various minerals, and fitting functions for estimating how long our reserves will last.",
  "version": "0.9.0",
  "engines": {
    "node": "v16.1.0",
    "npm": "7.6.3"
  },
  "keywords": [
    "data visualisation",
    "minerals",
    "reserves"
  ],
  "homepage": "https://github.com/peterhil/ninhursag",
  "author": "Peter Hillerstr√∂m <peter.hillerstrom@gmail.com>",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "lint": "eslint app/static/js",
    "start": "source ./activate && python run.py"
  },
  "private": true,
  "dependencies": {
    "d3-array": "^2.12.1",
    "d3-scale": "^3.3.0",
    "d3-shape": "^2.1.0",
    "events": "^3.3.0",
    "foundation-sites": "5.5.3",
    "humanize-plus": "1.5.0",
    "jquery": "2.1.4",
    "js-cookie": "^2.2.1",
    "less": "^3.13.1",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.0",
    "ramda": "0.27.1",
    "sass": "^1.47.0",
    "soundex-code": "0.1.2",
    "spin.js": "2.3.2",
    "tinycolor2": "1.1.2",
    "uglify-js": "^3.13.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "chokidar": "^3.5.1",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-svelte3": "^3.2.0",
    "rollup": "^2.44.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.37.0",
    "svelte-asyncable": "^2.0.2",
    "svelte-preprocess": "^4.7.3"
  }
}

  "engines": {
    "node": "v16.1.0",
    "npm": "7.6.3"
  },
  "keywords": [
    "data visualisation",
    "minerals",
    "reserves"
  ],
  "homepage": "https://github.com/peterhil/ninhursag",
  "author": "Peter Hillerstr√∂m <peter.hillerstrom@gmail.com>",
  "scripts": {
    "start": "source ./activate && python run.py"
  },
  "private": true,

{
  "name": "ninhursag",
  "description": "Ninhursag is a visualisation of US Geologic Survey statistics from year 1900 onwards of world production of various minerals, and fitting functions for estimating how long our reserves will last.",
  "version": "0.10.0",
  "engines": {
    "node": "v16.19.0",
    "npm": "9.3.0"
  },
  "keywords": [
    "data visualisation",
    "minerals",
    "reserves"
  ],
  "homepage": "https://github.com/peterhil/ninhursag",
  "author": "Peter Hillerstr√∂m <peter.hillerstrom@gmail.com>",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "lint": "eslint app/static/js",
    "start": ". ./activate && python run.py"
  },
  "private": true,
  "dependencies": {
    "d3-array": "^2.12.1",
    "d3-scale": "^3.3.0",
    "d3-shape": "^2.1.0",
    "debounce": "^1.2.1",
    "events": "^3.3.0",
    "humanize-plus": "1.5.0",
    "js-cookie": "^2.2.1",
    "milligram": "^1.4.1",
    "papaparse": "^5.3.0",
    "ramda": "0.27.1",
    "soundex-code": "0.1.2",
    "spin.js": "2.3.2",
    "tinycolor2": "1.1.2"
  },
  "devDependencies": {
    "@iconify/icons-carbon": "^1.2.15",
    "@iconify/svelte": "^3.1.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-eslint": "^9.0.3",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-terser": "^0.4.0",
    "carbon-icons-svelte": "^11.4.0",
    "chokidar": "^3.5.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
